{
    "name": "Flink SQL",
    "scopeName": "source.flinksql",
    "patterns": [
        {
            "include": "#comments"
        },
        {
            "include": "#strings"
        },
        {
            "include": "#numeric"
        },
        {
            "include": "#keywords"
        }
    ],
    "repository": {
        "comments": {
            "patterns": [
                {
                    "name": "comment.line.double-dash.flinksql",
                    "match": "--.*$"
                },
                {
                    "name": "comment.block.flinksql",
                    "begin": "/\\*",
                    "end": "\\*/",
                    "captures": {
                        "0": {
                            "name": "punctuation.definition.comment.flinksql"
                        }
                    }
                }
            ]
        },
        "strings": {
            "patterns": [
                {
                    "name": "string.quoted.single.flinksql",
                    "begin": "'",
                    "end": "'",
                    "patterns": [
                        {
                            "name": "constant.character.escape.flinksql",
                            "match": "''"
                        }
                    ]
                },
                {
                    "name": "string.quoted.double.flinksql",
                    "begin": "\"",
                    "end": "\"",
                    "patterns": [
                        {
                            "name": "constant.character.escape.flinksql",
                            "match": "\"\""
                        }
                    ]
                },
                {
                    "name": "string.quoted.backtick.flinksql",
                    "begin": "`",
                    "end": "`",
                    "patterns": [
                        {
                            "name": "constant.character.escape.flinksql",
                            "match": "``"
                        }
                    ]
                }
            ]
        },
        "numeric": {
            "patterns": [
                {
                    "name": "constant.numeric.flinksql",
                    "match": "\\b\\d+(\\.\\d+)?([eE][+-]?\\d+)?\\b"
                }
            ]
        },
        "keywords": {
            "patterns": [
                {
                    "name": "keyword.control.sql.standard.flinksql",
